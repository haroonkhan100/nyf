            <span ng-show="pieceDetailsForm.notes.$error.maxlength">Notes cannot be more than 150 characters.</span>
          </span>
        </div>
        <div class="control-group form-group col-md-12" ng-class="{'has-error':pieceDetails.smallThumbPath.$invalid && pieceDetails.smallThumbPath.$dirty}">
          <label>Small Thumb Path</label>
          <input type="text" class="form-control" name="smallThumbPath" ng-model="pieceDetailsForm.smallThumbPath" ng-maxlength="150" >
          <span class="help-block has-error" ng-if="pieceDetailsForm.smallThumbPath.$dirty">
