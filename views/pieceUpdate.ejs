      <br/>

      <!-- Disable signup button until the form has no errors -->
      <button class="btn btn-primary btn-lg btn-block col-md-12" type="submit" ng-disabled="pieceDetails.$invalid">
        <span ng-show="!pieceDetailsForm.loading">Update Piece</span>
        <span class="overlord-loading-spinner fa fa-spinner" ng-show="pieceDetailsForm.loading" ></span>
        <span ng-show="pieceDetailsForm.loading">Updating Piece...</span>
      </button>
      <button class="btn btn-danger btn-lg btn-block col-md-12" type="button" ng-click="deletePiece()" ng-disabled="pieceDetails.$invalid" >
        <span ng-show="!pieceDetailsForm.deleting">Delete Piece</span>
        <span class="overlord-loading-spinner fa fa-spinner" ng-show="pieceDetailsForm.deleting" ></span>
        <span ng-show="pieceDetailsForm.deleting">Deleting Piece...</span>
      </button>

      <input type="hidden" name="_csrf" value="<%= _csrf %>" />
